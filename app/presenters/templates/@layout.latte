<!DOCTYPE html>
<html>
<head>

	<!-- browsers -->
	<meta charset="UTF-8">
	<meta http-equiv="content-type" content="text/html; charset=UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta name="apple-mobile-web-app-capable" content="yes">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta http-equiv="cleartype" content="on">

	<!-- favicon -->
	<link type="image/x-icon" href="{$basePath}/favicon.ico" rel="shortcut icon">

	<!-- title -->
	<title n:snippet="title">{ifset title}{include title|stripHtml} | {/ifset}JUNI WALK</title>

	<!-- stylesheet -->
	{control assets:css frontend |indent|trim}
	<style type="text/css">
		.no-margin { margin: 0px; }
		.no-padding { padding: 0px; }
	</style>

</head>
<body n:block="body">

	<div class="navbar-wrapper">
		<nav class="navbar navbar-default navbar-fixed-top" role="navigation">
			<div class="container">
				<div class="navbar-header page-scroll">
					<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
						<span class="sr-only">Toggle navigation</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
					<a class="navbar-brand" n:href="Home:default">JUNI WALK</a>
				</div>
				<div id="navbar" class="navbar-collapse collapse">
					<ul class="nav navbar-nav navbar-right">
						<li><a class="page-scroll" n:href="Home:default">Home</a></li>
						<li><a class="page-scroll" n:href="Home:next">Next</a></li>
						<li><a class="page-scroll noajax" n:href=":Admin:Home:"><i class="fa fa-cog"></i></a></li>
						<li><a class="page-scroll" n:href=":Auth:signOut"><i class="fa fa-power-off"></i></a></li>
					</ul>
				</div>
			</div>
		</nav>
	</div>

	<div n:snippet="content" class="transition-auto transition-slide">

		{include content}

	</div>

	<!-- javascript -->
    {control assets:js frontend |indent|trim}

</body>
</html>